<h1>Show</h1>

<table>
  <tr>
    <th>Student ID</th>
    <th>First Name</th>
    <th>Last Name</th>
  </tr>
  <% @attendence.each do |attendence| %>
    <tr>
      <td><a href="/<%= @students.studentid %>"><%= @students.studentid %></a></td>
      <td><a href="/<%= @students.studentid %>"><%= @students.firstname %></a></td>
      <td><a href="/<%= @students.studentid %>"><%= @students.lastname %></a></td>
      <td><a href="/<%= @students.studentid %>"><%= attendence.status %></a></td>
    </tr>
  <% end %>
</table>

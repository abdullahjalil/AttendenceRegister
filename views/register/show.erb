<h1>Welcome Home</h1>

<button type="button" name="button">
  <a href="/students/generate_names">Fill Names</a>
</button>


  <table class="table">
    <thead class="thead-dark">
      <tr>
        <th scope="col">Student ID</th>
        <th scope="col">First Name</th>
        <th scope="col">Last Name</th>
        <th scope="col">Group ID</th>
      </tr>
    </thead>
    <tbody>
      <% @students.each do |student| %>
      <tr>
        <td><a href="/students/<%= student.studentid %>"><%= student.studentid %></a></td>
        <td><a href="/students/<%= student.groupid %>/<%= student.studentid %>"><%= student.firstname %></a></td>
        <td><a href="/students/<%= student.groupid %>/<%= student.studentid %>"><%= student.lastname %></a></td>
        <td><a href="/students/<%= student.groupid %>/<%= student.studentid %>"><%= student.groupid %></a></td>
      </tr>
      <% end %>

    </tbody>
  </table>

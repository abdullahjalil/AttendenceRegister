<h1>Welcome Home</h1>


  <table class="table">
    <thead class="thead-dark">
      <tr>
        <th scope="col">Group ID</th>
        <th scope="col">Group Name</th>
      </tr>
    </thead>
    <tbody>
      <% @groups.each do |group| %>
      <tr>
        <td><a href="/<%= group.groupid %>"><%= group.groupid %></a></td>
        <td><a href="/<%= group.groupid %>"><%= group.groupname %></a></td>
      </tr>
      <% end %>

    </tbody>
  </table>
